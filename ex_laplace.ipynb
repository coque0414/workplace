    "import cv2\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# 이미지 읽기\n",
    "image = cv2.imread('image.jpg')\n",
    "\n",
    "# 그레이스케일로 변환\n",
    "gray = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)\n",
    "\n",
    "# 라플라시안 필터 적용\n",
    "laplacian = cv2.Laplacian(gray, cv2.CV_64F)\n",
    "\n",
    "# 결과 이미지 출력\n",
    "plt.imshow(laplacian, cmap='gray')\n",
    "plt.axis('off')\n",
    "plt.show()"
